# GIMP sessionrc
# 
# This file takes session-specific info (that is info, you want to keep
# between two GIMP sessions).  You are not supposed to edit it manually, but
# of course you can do.  The sessionrc will be entirely rewritten every time
# you quit GIMP.  If this file isn't found, defaults are used.

(session-info "toplevel"
    (factory-entry "gimp-empty-image-window")
    (position 200 100)
    (size 640 480))
(session-info "toplevel"
    (factory-entry "gimp-single-image-window")
    (position 0 30)
    (size 1920 1050)
    (open-on-exit)
    (aux-info
        (left-docks-width "44")
        (right-docks-width "532")
        (maximized "no"))
    (gimp-toolbox
        (side left))
    (gimp-dock
        (side right)
        (book
            (current-page 1)
            (dockable "gimp-undo-history"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-tool-options"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))))
    (gimp-dock
        (side right)
        (position 203)
        (book
            (current-page 0)
            (dockable "gimp-color-editor"
                (tab-style automatic)
                (aux-info
                    (current-page "GimpColorSelect")))
            (dockable "gimp-palette-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true"))))
        (book
            (position 386)
            (current-page 0)
            (dockable "gimp-document-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true"))))
        (book
            (position 522)
            (current-page 0)
            (dockable "gimp-layer-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-channel-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-vectors-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true"))))))
(session-info "toplevel"
    (factory-entry "gimp-image-new-dialog")
    (position 768 402))
(session-info "toplevel"
    (factory-entry "gimp-toolbox-color-dialog")
    (position 739 335))
(session-info "toplevel"
    (factory-entry "gimp-file-export-dialog")
    (position 399 104)
    (size 801 694))
(session-info "toplevel"
    (factory-entry "gimp-preferences-dialog")
    (position 391 142)
    (size 1045 783))
(session-info "toplevel"
    (factory-entry "gimp-input-devices-dialog")
    (position 530 314))
(session-info "toplevel"
    (factory-entry "gimp-file-open-dialog")
    (position 379 180)
    (size 801 694))
(session-info "toplevel"
    (factory-entry "gimp-file-save-dialog")
    (position 558 192)
    (size 801 694))

(hide-docks no)
(single-window-mode yes)
(show-tabs yes)
(tabs-position 0)
(last-tip-shown 0)

# end of sessionrc
